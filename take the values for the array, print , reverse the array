#include<iostream>
using namespace std;

int main(){
int a[5];
cout<<"enter the array values"<<endl;
for(int i=0;i<5;i++){
    cin>>a[i];
    cout<" ";
}
cout<<endl;
cout<<"you have put this values in the array"<<endl;
for(int i=0;i<5;i++){
    cout<<a[i]<<" ";
}
cout<<endl;
cout<<"reverse of the array"<<endl;
for(int i=4; i>=0;i--){
    cout<<a[i]<<" ";
}}
