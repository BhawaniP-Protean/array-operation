#include<iostream>
using namespace std;
int main(){
    cout<<"enter the value for n in 2d array arr[n][m]"<<endl;
    int n;
    cin>>n;
    cout<<" enter the vcalue for m in 2d array arr[n][m]"<<endl;
    int m;
    cin>>m;
    int sum=0;

int arr[n][m];
cout<<"read the 2d array for "<<"arr["<<n<<"]["<<m<<"]"<<endl;
for(int i=0;i<n;i++){
    for(int j=0;j<m;j++){
        cin>>arr[i][j];
        cout<<" ";
    }cout<<endl;
}
cout<<"here is the values of arrays"<<endl;
for(int i=0;i<n;i++){
    for(int j=0;j<m;j++){
        cout<<arr[i][j];
        cout<<" ";
        sum=sum+arr[i][j];
    }cout<<endl;
} cout<<"the sum of the element of 2d array is"<<sum<<endl;
}
