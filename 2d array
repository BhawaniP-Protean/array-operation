#include<stdio.h>
int main()
{
    printf("enter the size of the array");
    int n,m;
    scanf("%d %d",&n,&m);
    printf("enter the element\n");
    int arr[n][m];
    for(int i=0; i<n; i++)
    {
        for(int j=0; j<m; j++)
        {
            printf("arr[%d][%d]",i,j);
            scanf("%d",&arr[i][j]);

        }
    }
    for(int i=0; i<n; i++)
    {
        for(int j=0; j<m; j++)
        {
            printf("%d",arr[i][j]);

        }
        printf("\n");
    }
}
